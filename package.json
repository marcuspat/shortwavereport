{
  "name": "shortwave-monitor",
  "version": "1.0.0",
  "description": "SPARC-enabled shortwave audio monitoring system with WebSDR integration",
  "main": "src/orchestrator.js",
  "type": "module",
  "scripts": {
    "start": "node src/orchestrator.js",
    "demo": "node demo.js",
    "dev": "node --watch src/orchestrator.js",
    "test": "node tests/run-tests.js",
    "test:unit": "node --test tests/unit/**/*.test.js",
    "test:integration": "node --test tests/integration/**/*.test.js",
    "test:e2e": "node --test tests/e2e/**/*.test.js",
    "test:coverage": "TEST_COVERAGE=true node tests/run-tests.js",
    "test:quick": "node run-quick-tests.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "test:verbose": "TEST_VERBOSE=true node tests/run-tests.js",
    "test:bail": "TEST_BAIL=true node tests/run-tests.js",
    "lint": "eslint src/**/*.js",
    "typecheck": "tsc --noEmit",
    "build": "npm run lint && npm run typecheck && npm run test:coverage",
    "sdr-discovery": "node src/agents/sdr-discovery.js",
    "audio-capture": "node src/agents/audio-capture.js",
    "audio-analysis": "node src/agents/audio-analysis.js",
    "report-generator": "node src/agents/report-generator.js"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "form-data": "^4.0.3",
    "helmet": "^8.1.0",
    "html-escaper": "^3.0.3",
    "joi": "^17.13.3",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "shortwave",
    "sdr",
    "websdr",
    "kiwisdr",
    "audio-monitoring",
    "sparc",
    "claude-flow"
  ],
  "author": "SPARC Orchestrator",
  "license": "MIT"
}
